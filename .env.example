# Superset Connection Configuration
# Copy this file to .env and fill in your actual values

# URL of your Superset instance (no trailing slash)
SUPERSET_BASE_URL=http://localhost:8088

# Superset credentials
# IMPORTANT: Use a dedicated service account with minimal required permissions
SUPERSET_USERNAME=your_username_here
SUPERSET_PASSWORD=your_password_here

# Optional: Superset API version (default: v1)
# SUPERSET_API_VERSION=v1

# Optional: Request timeout in seconds (default: 30)
# REQUEST_TIMEOUT=30

# Optional: Enable debug logging (default: false)
# DEBUG=false